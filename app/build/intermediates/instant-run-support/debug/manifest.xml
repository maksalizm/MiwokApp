<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pms.miwokapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="18"
        android:targetSdkVersion="24" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.pms.miwokapp.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.pms.miwokapp.NumbersActivity"
            android:label="@string/category_numbers" />
        <activity
            android:name="com.example.pms.miwokapp.PhrasesActivity"
            android:label="@string/category_phrases" />
        <activity
            android:name="com.example.pms.miwokapp.FamilyActivity"
            android:label="@string/category_family" />
        <activity
            android:name="com.example.pms.miwokapp.ColorsActivity"
            android:label="@string/category_colors" />
    </application>

</manifest>